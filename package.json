{
  "name": "whaleback",
  "author": "yangban",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "install": "concurrently \"pnpm run install:host\" \"pnpm run install:first\" ",
    "install:host": "cd host && pnpm install",
    "install:first": "cd first && pnpm install",
    "dev": "concurrently \"pnpm run dev:host\" \"pnpm run dev:first\" ",
    "dev:host": "cd host && pnpm run dev",
    "dev:first": "cd first && pnpm run dev",
    "build": "pnpm run build:host && pnpm run build:first",
    "build:host": "cd host && pnpm run build",
    "build:first": "cd first && pnpm run build",
    "preview": "concurrently \"pnpm run preview:host\" \"pnpm run preview:first\" ",
    "preview:host": "cd host && pnpm run preview",
    "preview:first": "cd first && pnpm run preview",
    "clean": "rm -rf node_modules host/node_modules remote/node_modules"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
